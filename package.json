{
  "name": "snatchers-monorepo-root",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm --prefix ./Snatchers_Backend/snatchers-backend-main start\" \"npm --prefix ./Snatchers_Frontend/snatchers.in-main start\"",
